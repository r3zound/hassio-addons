{
    "name": "Aria2",
    "version": "1.2.0",
    "slug": "aria2",
    "description": "Aria2 download manager",
    "url": "https://github.com/djhui/hassio-addons",
    "startup": "services",
    "hassio_role": "manager",
    "panel_icon": "mdi:download",
    "ingress": true,
    "ingress_port": 8080,
    "arch": [
        "armhf",
        "amd64",
        "aarch64",
        "i386"
    ],
    "boot": "auto",
    "options": {
        "options": []
    },
    "schema": {
        "downloads": "str?",
        "secret": "str?",
        "options": [
            {
                "name": "str",
                "value": "str"
            }
        ]
    },
    "ports": {
        "6800/tcp": 6800,
        "8080/tcp": 8080
    },
    "map": [
        "share:rw"
    ],
    "webui": "http://[HOST]:[PORT:8080]/webui-aria2/"
}