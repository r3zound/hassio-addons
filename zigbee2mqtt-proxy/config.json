{
  "name": "Zigbee2MQTT Proxy",
  "version": "0.2.0",
  "slug": "zigbee2mqtt_proxy",
  "description": "代理外部运行的Zigbee2MQTT。",
  "url": "https://gh-proxy.com/github.com/zigbee2mqtt/hassio-zigbee2mqtt/tree/master/zigbee2mqtt-proxy",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "init": true,
  "true": true,
  "ingress": true,
  "timeout": 30,
  "panel_icon": "mdi:zigbee",
  "map": [
    "config:ro"
  ],
  "ports": {
    "8099/tcp": null
  },
  "ports_description": {
    "8099/tcp": "Frontend tcp-listen port"
  },
  "options": {
    "server": null,
    "auth_token": ""
  },
  "schema": {
    "server": "url",
    "auth_token": "str?"
  },
  "image": "ghcr.nju.edu.cn/zigbee2mqtt/zigbee2mqtt-proxy-{arch}"
}
