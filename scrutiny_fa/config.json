{
  "apparmor": "true",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "Smartd 的智能监控（完全访问）Web 用户界面\n\n请注意，由于“Scrutiny”这个词在上下文中可能不是标准术语或特定于某个项目/产品，因此我将其直接翻译为“检查”。如果需要更准确的翻译，请提供更多的上下文信息。\n\n此外，“S.M.A.R.T monitoring”指的是使用S.M.A.R.T技术对硬盘驱动器进行监测。“WebUI”是“Web User Interface”的缩写，表示网页用户界面。而“Full Access”则意味着完全访问权限。",
  "environment": {
    "COLLECTOR_API_ENDPOINT": "http://localhost:8080",
    "COLLECTOR_HOST_ID": "home_assistant"
  },
  "full_access": true,
  "image": "ghcr.nju.edu.cn/alexbelgium/scrutiny-fa-{arch}",
  "ingress": true,
  "init": false,
  "map": [
    "share:rw",
    "addon_config:rw"
  ],
  "name": "Scrutiny (Full Access)",
  "options": {
    "Updates": "Hourly"
  },
  "panel_admin": false,
  "panel_icon": "mdi:glasses",
  "ports": {
    "8080/tcp": 8086,
    "8086/tcp": null
  },
  "ports_description": {
    "8080/tcp": "Web UI port",
    "8086/tcp": "InfluxDB admin"
  },
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "COLLECTOR_API_ENDPOINT": "str?",
    "COLLECTOR_HOST_ID": "str?",
    "Mode": "list(Collector+WebUI|Collector)?",
    "SMARTCTL_COMMAND_DEVICE_TYPE": "list(auto|ata|scsi|sat|usbcypress|usbjmicron|usbsunplus|marvell|megaraid|sntasmedia)?",
    "SMARTCTL_MEGARAID_DISK_NUM": "int?",
    "TZ": "str?",
    "Updates": "list(Quarterly|Hourly|Daily|Weekly|Custom)",
    "Updates_custom_time": "str?",
    "expose_collector": "bool?"
  },
  "slug": "scrutiny_fa",
  "udev": true,
  "url": "https://gh-proxy.com/github.com/AnalogJ/scrutiny",
  "version": "v0.8.1-9"
}
